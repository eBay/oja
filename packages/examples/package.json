{
  "name": "examples",
  "version": "2.0.2",
  "description": "Example of structuring app business logic using oja action with dependency injection",
  "main": "index.js",
  "scripts": {
    "test": "jest src",
    "coverage": "jest --collect-coverage src",
    "elint": "eslint --quiet -c .eslintrc *.js src/**/*.js",
    "lint": "yarn ojalint && yarn elint",
    "lint-fix": "eslint --fix *.js src/**/*.js"
  },
  "keywords": [
    "oja",
    "structure",
    "business",
    "logic",
    "flow"
  ],
  "author": "Dmytro Semenov <dimichsdg@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eBay/oja/issues"
  },
  "homepage": "https://github.com/eBay/oja/tree/master/packages/examples#readme",
  "dependencies": {
    "@ebay/oja-action": "^2.0.6",
    "@ebay/oja-context": "^2",
    "@ebay/oja-linter": "^2",
    "@ebay/oja-tools": "^2",
    "app-module-path": "^2.2.0",
    "express": "^4.16.1",
    "marko": "^4.4.28",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@marko/jest": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-ebay": "^1.0.0",
    "eslint-plugin-mocha": "^5.3.0",
    "hygen-oja-generators": "^2.0.8",
    "jest": "^24.9.0"
  },
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.spec.js"
    ],
    "moduleNameMapper": {
      "^(mock-data)/(.+)$": "<rootDir>/mock-data/$2",
      "^(src/)(.+)$": "<rootDir>/src/$2"
    },
    "moduleFileExtensions": [
      "js",
      "marko"
    ],
    "transform": {
      "\\.marko$": "@marko/jest"
    }
  },
  "private": true
}
